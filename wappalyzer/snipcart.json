{
    "name": "Snipcart",
    "website": "https://snipcart.com/",
    "matches": [
        {
            "search": "cookies[snipcart-cart]",
            "regexp": ".*"
        },
        {
            "regexp": "<link [^>]*href=\".*snipcart\\.css\""
        },
        {
            "regexp": "<div [^>]*id=\".*snipcart.*\""
        },
        {
            "search": "script",
            "regexp": "https://cdn\\.snipcart\\.com/themes/v([\\w.]+)/default/snipcart\\.js",
            "offset": 1
        }
    ]
}